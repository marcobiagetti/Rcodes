library(tseries)
library(zoo)
class("DATE")
db.ts<-zoo(db[,2:11],as.Date(db[,1],format="%d/%m/%Y")) 
#creating a zoo object with columns 2 through 11: date given by column 1 with a given date format
head(db.ts)
tail(db.ts)
db.ts<-na.locf(db.ts) #replacing NA with prior values
